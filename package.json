    "db:studio": "npx drizzle-kit studio",
    "db:generate": "npx drizzle-kit generate:pg",
    "db:migrate": "npx ts-node src/lib/db/migrate",
    "tailwind:css": "postcss src/public/styles/tailwind.css -o src/public/styles/style.css"
